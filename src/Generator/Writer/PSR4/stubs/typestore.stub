<?php

namespace DummyNamespace;

class TypeStore {

	private static $_cache = [];

	public static function getTypeDefinition($instanceOfType) {
		if (!array_key_exists($instanceOfType, self::$_cache)) {
			$this->_cache[$instanceOfType] = $instanceOfType::getTypeDefinition();
		}

		return $this->_cache[$instanceOfType];
	}

}